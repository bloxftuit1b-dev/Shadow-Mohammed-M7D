-- Gui to Lua
-- Version: 24.1 | The Fixed Settings Button Edition

local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")
local Players = game:GetService("Players")
local CoreGui = game:GetService("CoreGui")

local Player = Players.LocalPlayer
local PlayerGui = Player:WaitForChild("PlayerGui")

local LoadingConfig = {
    Duration = 6,
    FadeOutTime = 1.2,
    BarColor = Color3.fromRGB(138, 43, 226),
    ParticleCount = 80,
    ParticleSizeMax = 8,
    ParticleSpeed = 0.003,
    Version = "v24.1 | Fixed Settings"
}

local LoadingElements = {
    "جاري تهيئة الواجهة...",
    "التحقق من التحديثات...",
    "تحميل سكربتات التخريب...",
    "تجهيز قائمة السيارات...",
    "على وشك الانتهاء..."
}

local AnimationConnections = {}
local Particles = {}

local function CreateParticleSystem(parent)
    local particleFrame = Instance.new("Frame", parent)
    particleFrame.Name = "ParticleFrame"
    particleFrame.BackgroundTransparency = 1
    particleFrame.Size = UDim2.new(1, 0, 1, 0)
    particleFrame.ClipsDescendants = true

    for i = 1, LoadingConfig.ParticleCount do
        local particle = Instance.new("Frame")
        particle.Parent = particleFrame
        particle.BackgroundColor3 = LoadingConfig.BarColor
        particle.BorderSizePixel = 0
        local size = math.random(2, LoadingConfig.ParticleSizeMax)
        particle.Size = UDim2.new(0, size, 0, size)
        particle.Position = UDim2.new(math.random(), 0, math.random(), 0)
        particle.Transparency = math.random(4, 8) / 10
        local corner = Instance.new("UICorner", particle)
        corner.CornerRadius = UDim.new(1, 0)
        
        table.insert(Particles, particle)

        local connection = RunService.Heartbeat:Connect(function()
            if particle.Parent and particle.Visible then
                particle.Position = particle.Position + UDim2.new(0, 0, LoadingConfig.ParticleSpeed * (size / LoadingConfig.ParticleSizeMax), 0)
                if particle.Position.Y.Scale > 1.1 then
                    particle.Position = UDim2.new(math.random(), 0, -0.1, 0)
                end
            end
        end)
        table.insert(AnimationConnections, connection)
    end
end

local function CreateLoadingScreen()
    local ScreenGui = Instance.new("ScreenGui")
    ScreenGui.Name = "YuraHubLoadingScreen"
    ScreenGui.Parent = PlayerGui
    ScreenGui.ResetOnSpawn = false
    ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    ScreenGui.IgnoreGuiInset = true

    local Background = Instance.new("Frame", ScreenGui)
    Background.Name = "Background"
    Background.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    Background.BorderSizePixel = 0
    Background.Size = UDim2.new(1, 0, 1, 0)
    Background.BackgroundTransparency = 1

    local particleContainer = CreateParticleSystem(Background)

    local MainFrame = Instance.new("Frame", Background)
    MainFrame.Name = "MainFrame"
    MainFrame.AnchorPoint = Vector2.new(0.5, 0.5)
    MainFrame.Position = UDim2.new(0.5, 0, 0.5, 0)
    MainFrame.Size = UDim2.new(0, 0, 0, 0)
    MainFrame.BackgroundColor3 = Color3.fromRGB(28, 28, 36)
    MainFrame.BackgroundTransparency = 0.1
